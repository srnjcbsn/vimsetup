### URXVT ###

URxvt.scrollstyle:       plain
!! URxvt.font:              xft:Consolas:pixelsize=16
! URxvt.font:              xft:Hack:pixelsize=14
! URxvt.font:              xft:Source Code Pro:pixelsize=16
URxvt.font:              xft:Input Mono Narrow:style=light:pixelsize=16
! URxvt.font:              xft:Inconsolata-g for Powerline:pixelsize=18
URxvt.letterSpace:       -1
URxvt.saveLines:         100000
URxvt.scrollBar_right:   true
URxvt.scrollColor:       #1d2021
URxvt.urgentOnBell:      true
URxvt.scrollTtyOutput:   false
URxvt.scrollTtyKeypress: true

